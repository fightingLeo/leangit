mkdir learngit 
git remote  add origin git@github.com:fightingLeo/leangit.git
git pull origin master
git branch --set-upstream-to=origin/master master
git pull 

git rm --cached start   如果文件只执行了commit，没有被push 时想进行撤销操作时，执行此操作
git log --pretty=oneline
git reflog
git log --oneline
git reset --hard <hash>
git branch <branchname> 创建分支
git checkout <branchname> 切换分支
